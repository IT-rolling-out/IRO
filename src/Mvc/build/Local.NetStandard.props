<!-- Default setting for .net standard csproj. Import them if needed. 
     For other projects (xamarin, for example) can be used another settings. -->
<Project>
  
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <!-- Files, that you import later can override settings of previous files. 
       Local.Base.props must be imported last. -->
  <Import Project=".\Local.Base.props" />
</Project>